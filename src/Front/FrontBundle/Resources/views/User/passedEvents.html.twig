{% extends 'FrontFrontBundle::layout.html.twig' %}

{% block title %}
    {% trans %}user.passed_events{% endtrans %}
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('front/css/compiled/profile-portfolio.css') }}" />       
{% endblock %}  



{% block content %}

    {% include 'FrontFrontBundle:User:userBreadcrumb.html.twig' with { title : 'user.passed_events'|trans } %}


    <!--=== Content Part ===-->
    <div class="container content profile">

        <div class="row">
            <!-- Profile Content -->            
            <div class="col-md-12">

                <div class="profile-body">

                    <!-- Portfolio Sorting Blocks -->
                    <div class="sorting-block">
                        {{ render_esi(path('front_musictype_filters')) }}

                        <!-- Easy Blocks v1 -->                
                        <div class="row high-rated margin-bottom-20 sorting-grid">

                            {% for event in pagination %}
                                {% include 'FrontFrontBundle:Event:eventWithButtons.html.twig' %}
                            {% endfor %}
                        </div>
                        <!-- End Easy Blocks v1 --> 
                        <div class="clearfix"></div>
                    </div>
                    <!-- End Portfolio Sorting Blocks -->   

                </div>

                <div class='text-center'>
                    {{ knp_pagination_render(pagination) }}
                </div>

            </div>
            <!-- End Profile Content -->            
        </div>
    </div>
    <!-- End Profile Content -->



</div><!--/container-->	 	
<!--=== End Content Part ===-->

{% endblock %}


{% block bottomJavascripts %}
    {{ parent() }}
    <script src="{{ asset("front/js/compiled/page-portfolio.js") }}"></script> 

    <script type="text/javascript">
        jQuery(document).ready(function () {
            App.init();
            PortfolioPage.init();

        });
    </script>

{% endblock %}    