<tr>
    <td>
        {% include 'FrontFrontBundle:User:memberPhoto.html.twig' with { user : person, class : 'rounded-x' } %}
    </td>
    <td class="td-width">
        <h3><a href="{{ path('front_user_show_public', { username : person.username, id : person.id } ) }}">{{ person.username|capitalize }}</a></h3>
        <small class="hex">{{ person.getBaseline|capitalize }}</small>
        <p>{{ person.getDescriptionShort|raw }}</p>
    </td>
    <td>
        {% include 'FrontFrontBundle:User:musicTypeList.html.twig' %}
    </td>
    <td>
        {% include 'FrontFrontBundle:User:socialIconList.html.twig' %}
    </td>
    {% render(path('front_user_show_overviews_index', { id : person.id } )) %}
</tr>