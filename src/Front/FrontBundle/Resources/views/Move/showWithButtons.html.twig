<div id="move_{{ move.id }}">
{% set video = move.getVideo() %}
{% if video %}
    {% include 'FrontFrontBundle:Video:show.html.twig' with { video : video } %}
{% endif %}
<span>{{ move.getTitle() }}</span>
<div class="col-md-2 md-margin-bottom-20">
        {% if app.user is not null and app.user == move.getUser() and is_granted('ROLE_USER') %} 
            <p> 
                <button class="modifyMove btn-u btn-u-green" data-move-id="{{ move.id }}" href="{{ path('front_move_edit', { 'id': move.id }) }}">
                    <i class='fa fa-pencil'></i>
                </button> 
            </p>
            <p>
                <button class="deleteMove btn-u btn-u-red"  data-move-id="{{ move.id }}"  href="{{ path('front_move_delete', { 'id': move.id }) }}">
                    <i class='fa fa-times'></i>
                </button>
            </p>
        {% endif %}   
    </div>
</div>