

<li class="dropdown mega-menu-fullwidth">
    <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown">
        {% trans %}navbar.meet{% endtrans %}
    </a>
    <ul class="dropdown-menu">
        <li>
            <div class="mega-menu-content disable-icons">
                <div class="container">
                    <div class="row equal-height">

                        <div class="col-md-3 equal-height-in ">
                            <ul class="list-unstyled equal-height-list">
                                <li class='sm-hidden'>
                                    <h3>Rechercher</h3>
                                </li>
                                <li>
                                    {{ form_start(filterForm, { 'attr' : { 'class' : 'no_validation', 'id' : 'user_filter_form' } } ) }}
                                    <div class="input-group m-l-10 m-r-10">
                                        {% set placeholder %}
                                            {% trans %}user.search_input_placeholder{% endtrans %}
                                        {% endset %}
                                        {{ form_widget(filterForm.search,{ 'attr' : { 'class' : 'form-control', 'placeholder' : placeholder|trim } } ) }}
                                        <span class="input-group-btn">
                                            <button class="btn-u" type="submit"><i class="fa fa-search"></i></button>
                                        </span>
                                        {{ form_row(filterForm.musictype) }}
                                        {{ form_row(filterForm.usertype) }}
                                    </div>
                                    {{ form_end(filterForm) }}
                                </li>
                            </ul>
                        </div> 

                        <div class="col-md-3 equal-height-in">
                            <ul class="list-unstyled equal-height-list">
                                <li class='sm-hidden'>
                                    <h3>{% trans %}navbar.meet.salsa{% endtrans %}</h3>
                                </li>

                                <li>
                                    <a href="{{ path('front_user_filter', {'user_filter[musictype]':  'Salsa' , 'user_filter[usertype]':  'artist'  , 'user_filter[_token]' :  csrf_token('user_filter')  } ) }}">
                                        {% trans %}navbar.meet.salsa.artist{% endtrans %}
                                    </a>
                                </li>
                                <li>
                                    <a href="{{ path('front_user_filter', {'user_filter[musictype]':  'Salsa' , 'user_filter[usertype]':  'teacher'  , 'user_filter[_token]' :  csrf_token('user_filter')  } ) }}">
