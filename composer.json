{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-4": { "": "src/" }
    },
    "require": {
        "php": ">5.5,<=5.6.20",
        "symfony/symfony": "2.7.9",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4",
        "symfony/assetic-bundle": "^2.7",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "2.8.2",
        "sensio/distribution-bundle": "~4.0",
        "sensio/framework-extra-bundle": "3.0.13",
        "incenteev/composer-parameter-handler": "~2.0",
        "twig/extensions": "^1.3",
        "friendsofsymfony/user-bundle": "^1.3",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "hwi/oauth-bundle": "0.4.0",
        "jms/i18n-routing-bundle": "^2.0",
        "jms/di-extra-bundle": "1.7.1",
        "knplabs/doctrine-behaviors": "1.3.0",
        "a2lix/translation-form-bundle": "2.1.1",
        "punkave/symfony2-file-uploader-bundle": "dev-master",
        "phpunit/phpunit": "4.8.23",
        "willdurand/geocoder-bundle": "^4.0",
        "beberlei/DoctrineExtensions": "1.0.6",
        "facebook/php-sdk-v4": "5.1.2",
        "liip/imagine-bundle": "1.5.2",
        "knplabs/knp-paginator-bundle": "2.5.1",
        "jms/translation-bundle": "1.1.0",
        "elao/form-translation-bundle": "1.4.*"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "www",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.7-dev"
        }
    }
}
